{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "double"],
      "react/react-in-jsx-scope": "off",
      "no-trailing-spaces": "error"
    }
  }